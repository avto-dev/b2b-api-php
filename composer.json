{
    "name": "avto-dev/b2b-api-php",
    "description": "PHP package for a working with B2B API service",
    "keywords": [
        "php",
        "b2b",
        "client"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develops",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\B2BApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\B2BApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'"
    },
    "suggest": {
        "avto-dev/b2b-api-php-laravel": "Laravel package for a working with B2B API client."
    },
    "support": {
        "issues": "https://github.com/avto-dev/b2b-api-php/issues",
        "source": "https://github.com/avto-dev/b2b-api-php"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
