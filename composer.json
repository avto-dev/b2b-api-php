{
  "name": "avto-dev/b2b-api-php",
  "description": "PHP package for a working with B2B API service",
  "keywords": ["php", "b2b", "client"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Avto Develop",
      "homepage": "https://github.com/avto-dev"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7 || ^6.4",
    "symfony/var-dumper": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "AvtoDev\\B2BApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AvtoDev\\B2BApi\\Tests\\": "tests"
    }
  },
  "suggest": {
    "hirak/prestissimo": "composer parallel install plugin"
  },
  "support": {
    "issues": "https://github.com/avto-dev/b2b-api-php/issues",
    "source": "https://github.com/avto-dev/b2b-api-php"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --no-coverage"
  },
  "config": {
    "sort-packages": true
  }
}


